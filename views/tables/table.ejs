    <div class="w3-container w3-teal">
        <h1>Table Position</h1>
    </div>

    <div class="w3-row-padding w3-margin-top">
        <%tables.forEach(table=>{%>
        <div class="w3-third">
            <div class="w3-card">
                <div class="w3-container">
                    <h5><a href="/item/<%=table.id%>">
                            <%=table.name%>
                        </a>
                    </h5>
                </div>
            </div>
        </div>
        <%})%>



